<% @articles.each do |article| %>
<div class="card m-3">
  <div class="card-header">
   <%= link_to article.title, article %>
  </div>
  <div class="card-body">
    <h5 class="card-title"></h5>
    <p class="card-text"><%= article.body%></p>
    <%= link_to "See more", article, class: "btn btn-primary"%>

  </div>
</div>
<% end %>